{
    "name": "mediatr-ts",
    "version": "0.3.0",
    "description": "Mimic the famous MediatR csharp library see: (https://github.com/jbogard/MediatR)",
    "main": "lib/index.js",
    "scripts": {
        "start": "ts-node src/index.ts",
        "test": "rimraf lib && jest",
        "build": "rimraf lib && tsc --project tsconfig.production.json && tsc-alias",
        "prepare": "npm run build",
        "test:coverage": "npm test -- --coverage",
        "test:badges": "npm run test:coverage && jest-coverage-badges --output badges"
    },
    "keywords": [
        "mediatr",
        "mediator",
        "inversify",
        "pattern"
    ],
    "author": "m4ss1m0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/m4ss1m0g/mediatr-ts.git"
    },
    "devDependencies": {
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.3",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "eslint": "^8.21.0",
        "inversify": "^6.0.1",
        "jest": "^28.1.3",
        "jest-coverage-badges": "^1.1.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.7.0",
        "typescript": "^4.7.4"
    },
    "files": [
        "lib/*"
    ]
}
